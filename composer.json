{
    "name": "th3mouk/contact-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Th3MoukContactBundle to factorize contact form generation throught websites.",
    "keywords": [
        "contact", "contact mailer", "symfony", "bundle"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jérémy Marodon",
            "email": "marodon.jeremy@gmail.com"
        }
    ],
    "require": {
        "symfony/symfony": "^2.3",
        "th3mouk/mailer": "^1.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "^0.5|^1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Th3Mouk\\ContactBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    }
}
